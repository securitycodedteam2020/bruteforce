def slowness(s):
  try:
    for w in s + '\n' :
      sys.stdout.write(w)
      sys.stdout.flush()
      time.sleep(5. / 100)
    print('\n')
#       time.sleep(1)
  except KeyboardInterrupt:
    time.sleep(1)
    slowness('Exiting =)')
    print('\n')
    sys.exit(0)


